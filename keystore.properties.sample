# Configuración del keystore para firma de APK de producción
# Copia este archivo como keystore.properties y completa los valores

# Ruta al archivo keystore (relativa al directorio app/)
storeFile=app/levelup-release.keystore

# Contraseña del keystore
storePassword=TU_CONTRASEÑA_AQUI

# Alias de la clave
keyAlias=levelup-key

# Contraseña de la clave
keyPassword=TU_CONTRASEÑA_AQUI

